{
  "name": "pumptrack",
  "version": "1.0.0",
  "description": "Minimal app for tracking your exercises, habits and other darlings of yours (or just pumps)",
  "author": "capthiron",
  "license": "MIT",
  "private": false,
  "browserslist": "last 2 Safari versions, last 2 Chrome versions",
  "scripts": {
    "start": "parcel public/index.html --dist-dir build",
    "build": "parcel build public/index.html --dist-dir build",
    "prod": "yarn build && serve -s build",
    "test": "elm-test"
  },
  "staticFiles": {
    "staticPath": "public/assets",
    "distDir": "build/assets"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.4.1",
    "@parcel/transformer-elm": "2.4.1",
    "@parcel/transformer-webmanifest": "2.4.1",
    "elm": "^0.19.1-5",
    "elm-test": "^0.19.1-revision7",
    "parcel": "^2.4.1",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "postcss": "^8.4.12",
    "serve": "^13.0.2",
    "tailwindcss": "^3.0.24",
    "yarn": "^1.22.18"
  },
  "dependencies": {
    "tailwind-nord": "^1.2.0"
  }
}
